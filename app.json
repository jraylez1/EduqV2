{
    "expo": {
        "name": "EduQ",
        "slug": "EduQ",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/images/app-icon-all.png",
        "userInterfaceStyle": "light",
        "splash": {
            "image": "./assets/images/splash-logo-all.png",
            "resizeMode": "contain",
            "backgroundColor": "#023468"
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "requireFullScreen": true,
            "supportsTablet": true,
            "permissions": {
                "microphone": true,
                "mediaLibrary": true
            }
        },
        "android": {
            "permissions": ["android.permission.RECORD_AUDIO", "android.permission.WRITE_EXTERNAL_STORAGE"],
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/splash-logo-all.png",
                "backgroundColor": "#ffffff"
            },
            "package": "com.qna.eduq"
        },
        "web": {
            "favicon": "./assets/favicon.png"
        },
        "plugins": [
            [
                "expo-build-properties",
                {
                    "android": {
                        "usesCleartextTraffic": true
                    }
                }
            ],
            [
                "expo-image-picker",
                {
                    "photosPermission": "custom photos permission"
                }
            ],
            [
                "expo-screen-orientation",
                {
                    "initialOrientation": "PORTRAIT_UP"
                }
            ]
        ],
        "extra": {
            "eas": {
                "projectId": "99f25fff-89d0-45c2-a902-2d18dc95f22e"
            }
        }
    }
}
